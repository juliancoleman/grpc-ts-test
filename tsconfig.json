{
  "compilerOptions": {
    "outDir": "build",
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "types": [
      "node",
      "grpc"
    ],
    "typeRoots": [
      "./src/generated/"
    ]
  },
  "include": [
    "src/**/**.ts",
  ],
  "exclude": [
    "node_modules"
  ]
}
